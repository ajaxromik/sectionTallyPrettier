{
	"manifest_version": 3,
	"name": "Prettier Section Tally",
	"description": "Runs a script on Rowan Section Tally automatically when the page loads.",
	"version": "1.0.1",
	"icons": {
	  "16": "icons/icon-16.png",
	  "32": "icons/icon-32.png"
	},
	"content_scripts": [
	  {
		"js": [
			"scripts/jquery-3.7.1.min.js",
			"scripts/select2.min.js",
			"scripts/sectionTallyPrettier.js"
		],
		"css": [
			"css/select2.min.css"
		],
		"matches": ["https://banner.rowan.edu/reports/*"]
	  }
	]
  }
